<h1>Listing account_payables</h1>

<table>
  <tr>
    <th>Time</th>
    <th>Debit</th>
    <th>Credit</th>
    <th>Description</th>
    <th>Date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @account_payables.each do |account_payable| %>
  <tr>
    <td><%= account_payable.time %></td>
    <td><%= account_payable.debit %></td>
    <td><%= account_payable.credit %></td>
    <td><%= account_payable.description %></td>
    <td><%= account_payable.date %></td>
    <td><%= link_to 'Show', account_payable %></td>
    <td><%= link_to 'Edit', edit_account_payable_path(account_payable) %></td>
    <td><%= link_to 'Destroy', account_payable, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

