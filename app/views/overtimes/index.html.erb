<% content_for :bottom_js do %>
  <%= javascript_include_tag "jquery.inputmask" %>
  <%= javascript_include_tag "jquery.inputmask.date.extensions" %>
  <%= javascript_include_tag "jquery.inputmask.extensions" %>
  <%= javascript_include_tag "call_datepicker" %>
  <%= javascript_include_tag "bootstrap-timepicker.min" %>
  <%= javascript_include_tag "call_timepicker" %>
  <%= javascript_include_tag "form_absent" %>
<% end%>

<% content_for :content_title do %>
  Gaji
<% end%>

<% content_for :content_sub_title do %>
  Data Riwayat Lembur
<% end%>

<% content_for :content_body do %>

  <%= render partial: "search_overtime" if current_user.is_admin? %>

  <%= link_to "<< sebelumnya", overtimes_path(:year => @prev.year, :month => @prev.month), class: "btn btn-info" %>
  <%= @start_date.strftime("%B ") %>
  <%= link_to "selanjutnya >>", overtimes_path(:year => @next.year, :month => @next.month), class: "btn btn-info" %>
  <br />
  <br />

  <%= render partial: "list" %>
<% end %>

<% content_for :content_nav do %>
  <%= link_to 'Buat Data Lembur', new_overtime_path, :class=>'btn btn-primary' %>
<% end%>


