<%= form_tag overtimes_path(:year => @start_date.year, :month => @start_date.month), :method => 'get' do %>
<table>
	<tr>
		<td>
			<div class="form-group">
				<%= select_tag(:user_id, options_for_select(@users), class: "form-control") %>
			</div>
		</td>
		<td>
			<div class="form-group">
				<%= submit_tag "Cari", :name => nil, :class=>'btn btn-default' %>
			</div>
		</td>
	</tr>
</table>


<% end %>