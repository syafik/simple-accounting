<% content_for :bottom_js do %>
    <%= javascript_include_tag "jquery.inputmask"%>
    <%= javascript_include_tag "jquery.inputmask.extensions"%>
    <%= javascript_include_tag "call_datepicker" %>
<% end%>


<% content_for :content_title do %>
Karyawan
<% end%>

<% content_for :content_sub_title do %>
Edit Data Karyawan
<% end%>

<% content_for :content_body do %>
<%= form_for(@manage_user, :url => {action: "update", controllers: "manage_users"}) do |f| %>
<%= render partial: 'form', locals: {f: f,  btn_title: "Ubah Data", from: "edit"} %>
<% end %>
<% end%>



<% content_for :content_nav do %>
<%= link_to 'Lihat', manage_user_path(@manage_user), :class=>'btn btn-primary'%> 
<% if current_user.role_id == 2%>
<%= link_to 'Kembali', manage_users_path, :class=>'btn btn-primary' %>
<% else %>
<%= link_to 'Kembali', manage_user_path(current_user.id), :class=>'btn btn-primary' %>
<% end%>
<% end%>


